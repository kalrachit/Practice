
public class BubbleSort {
	
	public Integer[] bubbleSort(Integer[] array, int x){
		//Integer[] sortedarray = new Integer[x];
	if (array!= null){
		int tmp =0;
		for (int i =0; i<x-1; i++){
			for (int j =1; j<x; j++){
				if (array[i] > array[j]){
					tmp =array[i];
					array[i] = array[j];
					array[j] =tmp;
				}
		
			}
			
		}
	}
				
		return array;
	}
	
	public static void main(String args[]){
		Integer[] arr = new Integer[10];
		arr[0]=10;
		arr[1]=6;
		arr[2]=7;
		arr[3]=8;
		arr[4]=9;
		arr[5]=3;
		arr[6]= 2;
		
		BubbleSort bs = new BubbleSort();
		arr=bs.bubbleSort(arr, arr.length);
		for(int i=0; i< arr.length; i++)
			System.out.println(arr[i]);
		

	}
	
	

}
