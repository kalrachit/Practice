import java.util.LinkedList;
import java.util.Queue;

class Node 
{
    int data;
    Node left, right;
  
    Node(int item) 
    {
        data = item;
        left = right = null;
    }
}
  
public class BinaryTreeMax {
	Node root;
	int max = 0;
	public int Findmax(){
		{
			if (root == null)
				return max;
			
			if (root.left != null){
				if(root.left.data >= max){
					max = root.left.data;
				}
			}
			if (root.right != null ){
				if(root.right.data >= max){
					max = root.right.data;
				}
			}
			
		}Findmax();
		            
		  
	return max;
	}
}
