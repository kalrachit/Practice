import java.util.LinkedList;
import java.util.Queue;


public class deepestNodeinBT {
	
	private Node findLastNode(Node root){
		Node tmp= null;
		if(root ==null)
			return null;
		Queue<Node> q = new LinkedList<Node>();
		q.offer(root);
		while(!q.isEmpty()){
			tmp = q.poll();

				if (tmp.left != null){
					q.offer(tmp.left);}
				if (tmp.right!= null){
					q.offer(tmp.right);}
				}
			return tmp;
	}
	
	public static void main(String args[]){
		
	}

}
