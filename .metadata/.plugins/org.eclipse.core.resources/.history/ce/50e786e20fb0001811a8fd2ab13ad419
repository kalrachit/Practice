
public class findingSizebyStack {
	
	
	
	public int findSize(Node root){
		if(root==null)
			return 0;
		int leftdep = findSize(root.left);
		int rightdep = findSize(root.right);
		
		
		return(leftdep > rightdep)? leftdep+1 : rightdep +1;
	}
	
	public int findSizebyStack(Node root){
		
		
		
		return 0;
	}
	
	
	
	
	public static void main(String args[]){
		
	}
	

}
